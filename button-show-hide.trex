<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" xmlns="http://www.tableau.com/xml/extension_manifest">
  <dashboard-extension id="com.tableau.extensions.example" extension-version="0.1.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>This extension is written exclusively for the LogixReports dashboard. It uses parmaters (also in URL) to show or hide the navigation buttons.</description>
    <author name="Dan Girard" email="dgirard@logixhealth.com" organization="LogixHealth, Inc." website="https://www.logixhealth.com"/>
    <min-api-version>0.1</min-api-version>
    <source-location>
      <url>https://cdnbi.logixhealth.com/extensions/button-show-hide/</url>
    </source-location>
    <icon></icon>
  </dashboard-extension>
  <resources>
    <resource id="name">
      <text locale="en_US">LogixReports Button Show/Hide</text>
    </resource>
  </resources>
</manifest>